-- Scripts:

local function MRCB_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	local button = script.Parent
	
	-- Your loadstring script (Make sure the code inside is safe!)
	local scriptToExecute = [[
	    loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"))()
	]]
	
	button.MouseButton1Click:Connect(function()
		local func = loadstring(scriptToExecute)
		if func then
			func() -- Executes the script
		else
			warn("Loadstring failed!")
		end
	end)
	
end
coroutine.wrap(MRCB_fake_script)()
